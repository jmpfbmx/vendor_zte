<permissions>

<privapp-permissions package="com.qualcomm.location">
    <permission name="android.permission.CONTROL_LOCATION_UPDATES"/>
</privapp-permissions>

<permission name="com.qualcomm.permission.IZAT" >
    <group gid="gps" />
    <group gid="net_raw" />
    <group gid="net_admin" />
    <group gid="system" />
    <group gid="diag" />
    <group gid="oem_2950" />
    <group gid="sdcard_rw" />
    <group gid="media_rw" />
    <group gid="wifi" />
</permission>

</permissions>
